    "@types/lodash": "^4.14.191",
    "@types/mapbox-gl": "^2.7.11",
    "@types/node": "^18.8.2",
    "@types/react": "^18.0.15",
    "@types/react-burger-menu": "^2.8.3",
    "@types/react-dom": "^18.0.6",
    "@types/react-slick": "^0.23.10",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.1",
    "happy-dom": "^7.5.6",
    "postcss": "^8.4.16",
    "prisma": "^4.8.1",
    "react-is": "^17.0.2",
    "tailwindcss": "^3.1.8",
